<%= form_for @inner_category, :url => (action == "new") ? admin_inner_categories_path : admin_inner_category_path do |f| %>
		<%# if params[:action] == "create" %>
				<%= render 'admin/errors/error_message', model: @inner_category %>
		<%# end %>
  <p>

    <%= f.label :sub_category_id %><br />
				<%= f.select :sub_category_id,SubCategory.all.collect {|p| [ p.name, p.id ] }, :prompt => 'Select Sub Category' %>
  </p>  
<br/>		
		 <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
		<br/>
		<p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
		<br/>
		<br />

  <p><%= f.submit "Submit" ,:id=>"inner_category_submit"%></p>
<% end %>

